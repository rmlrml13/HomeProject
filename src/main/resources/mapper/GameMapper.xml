<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
                  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.study.home.mapper.GameMapper">

	<select id="gameList" resultType="org.study.home.model.GameDTO">
		SELECT * from gameTBL
	</select>
	
	<select id="gameRead" resultType="org.study.home.model.GameDTO">
		SELECT * from gameTBL where game_no = #{game_no}
	</select>


</mapper>